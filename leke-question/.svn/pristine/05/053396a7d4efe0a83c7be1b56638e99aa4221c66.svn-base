package cn.strong.leke.question.model.question.query;

import java.io.Serializable;
import java.util.List;

/**
 *
 * 描述: 随机获取习题查询
 *
 * @author raolei
 * @created 2016年10月12日 下午6:59:26
 * @since v1.0.0
 */
public class RandomGetQuestionQuery implements Serializable {

	/**
	 * Description:
	 */
	private static final long serialVersionUID = -3073211890834795981L;

	private List<Long> questionIds;// 排除习题ID
	private Long materialNodeId; // 教材节点ID
	private List<Long> knowledgeIds;// 知识点ID
	private Long questionTypeId;// 习题类型
	private Double difficulty;// 习题难易度

	public List<Long> getQuestionIds() {
		return questionIds;
	}

	public void setQuestionIds(List<Long> questionIds) {
		this.questionIds = questionIds;
	}

	public Long getMaterialNodeId() {
		return materialNodeId;
	}

	public void setMaterialNodeId(Long materialNodeId) {
		this.materialNodeId = materialNodeId;
	}

	public List<Long> getKnowledgeIds() {
		return knowledgeIds;
	}

	public void setKnowledgeIds(List<Long> knowledgeIds) {
		this.knowledgeIds = knowledgeIds;
	}

	public Long getQuestionTypeId() {
		return questionTypeId;
	}

	public void setQuestionTypeId(Long questionTypeId) {
		this.questionTypeId = questionTypeId;
	}

	public Double getDifficulty() {
		return difficulty;
	}

	public void setDifficulty(Double difficulty) {
		this.difficulty = difficulty;
	}

}
