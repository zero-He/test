package cn.strong.leke.question.model.wrongquestion;


/**
 * 班级学科错题知识点分布数量
 * @author Zhang Fujun
 * @date 2017年1月11日
 */
public class WrongSubjectKnowledge {


	private Long knowledgeId;
	private String knowledgeName;
	private Long questionTotal;
	/**
	 * @return the knowledgeId
	 */
	public Long getKnowledgeId() {
		return knowledgeId;
	}
	/**
	 * @param knowledgeId the knowledgeId to set
	 */
	public void setKnowledgeId(Long knowledgeId) {
		this.knowledgeId = knowledgeId;
	}
	/**
	 * @return the knowledgeName
	 */
	public String getKnowledgeName() {
		return knowledgeName;
	}
	/**
	 * @param knowledgeName the knowledgeName to set
	 */
	public void setKnowledgeName(String knowledgeName) {
		this.knowledgeName = knowledgeName;
	}
	/**
	 * @return the questionTotal
	 */
	public Long getQuestionTotal() {
		return questionTotal;
	}
	/**
	 * @param questionTotal the questionTotal to set
	 */
	public void setQuestionTotal(Long questionTotal) {
		this.questionTotal = questionTotal;
	}
	
	
}
