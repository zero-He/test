package cn.strong.leke.homework.model.mongo;

import java.math.BigDecimal;
import java.util.List;

/**
 * 自主练习 题目信息
 * @author Zhang Fujun
 * @date 2016年11月6日
 */
public class ExerciseQuestionResult {
	
	private Long questionId;
	
	/**
	 * 是否正确
	 */
	private Boolean isCorrect;
	
	/**
	 * 正确率
	 */
	private BigDecimal correctRate;
	
	/**
	 * 该题目的作答信息
	 */
	
	private List<String> answerContent;
	/**
	 * 该题目的批改信息
	 */
	private List<ExerciseQuestionCorrect> correctContent;
	
	private List<ExerciseQuestionResult> subs;
	
	/**
	 * @return the questionId
	 */
	public Long getQuestionId() {
		return questionId;
	}
	/**
	 * @param questionId the questionId to set
	 */
	public void setQuestionId(Long questionId) {
		this.questionId = questionId;
	}
	/**
	 * @return the answerContent
	 */
	public List<String> getAnswerContent() {
		return answerContent;
	}
	/**
	 * @param answerContent the answerContent to set
	 */
	public void setAnswerContent(List<String> answerContent) {
		this.answerContent = answerContent;
	}
	/**
	 * @return the correctContent
	 */
	public List<ExerciseQuestionCorrect> getCorrectContent() {
		return correctContent;
	}
	/**
	 * @param correctContent the correctContent to set
	 */
	public void setCorrectContent(List<ExerciseQuestionCorrect> correctContent) {
		this.correctContent = correctContent;
	}
	/**
	 * @return the subs
	 */
	public List<ExerciseQuestionResult> getSubs() {
		return subs;
	}
	/**
	 * @param subs the subs to set
	 */
	public void setSubs(List<ExerciseQuestionResult> subs) {
		this.subs = subs;
	}
	/**
	 * @return the isCorrect
	 */
	public Boolean getIsCorrect() {
		return isCorrect;
	}
	/**
	 * @param isCorrect the isCorrect to set
	 */
	public void setIsCorrect(Boolean isCorrect) {
		this.isCorrect = isCorrect;
	}
	/**
	 * @return the correctRate
	 */
	public BigDecimal getCorrectRate() {
		return correctRate;
	}
	/**
	 * @param correctRate the correctRate to set
	 */
	public void setCorrectRate(BigDecimal correctRate) {
		this.correctRate = correctRate;
	}
	
}
