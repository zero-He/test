package cn.strong.leke.homework.model.query;

import java.util.List;

/**
 * 批量布置作业
 * 根据班级和学科查询老师
 * @author Zhang Fujun
 * @date 2016年7月22日
 */
public class AssignMatchingTeacherQuery {

	private List<Long> subjectIds;

	private List<AssignMatchingStuClass> stuClasses;

	/**
	 * @return the subjectIds
	 */
	public List<Long> getSubjectIds() {
		return subjectIds;
	}

	/**
	 * @param subjectIds the subjectIds to set
	 */
	public void setSubjectIds(List<Long> subjectIds) {
		this.subjectIds = subjectIds;
	}

	/**
	 * @return the stuClasses
	 */
	public List<AssignMatchingStuClass> getStuClasses() {
		return stuClasses;
	}

	/**
	 * @param stuClasses the stuClasses to set
	 */
	public void setStuClasses(List<AssignMatchingStuClass> stuClasses) {
		this.stuClasses = stuClasses;
	}
}
