package cn.strong.leke.homework.model;

import java.util.Date;

public class HolidayHwMQ implements java.io.Serializable {
	private static final long serialVersionUID = 3317031027757707087L;

	public HolidayHwMQ() {

	}

	public HolidayHwMQ(Integer year, Integer holiday, Date hwStartTime, Date hwCloseTime, Date start, Date end) {
		super();
		this.year = year;
		this.holiday = holiday;
		this.hwStartTime = hwStartTime;
		this.hwCloseTime = hwCloseTime;
		this.start = start;
		this.end = end;
	}

	//设置信息
	private Integer year;
	//1:寒假，2：暑假
	private Integer holiday;

	//作业的开始和截止时间
	private Date hwStartTime;
	private Date hwCloseTime;

	//过滤周错题作业的时间段
	private Date start;
	private Date end;

	/**
	 * @return the year
	 */
	public Integer getYear() {
		return year;
	}

	/**
	 * @param year the year to set
	 */
	public void setYear(Integer year) {
		this.year = year;
	}

	/**
	 * @return the holiday
	 */
	public Integer getHoliday() {
		return holiday;
	}

	/**
	 * @param holiday the holiday to set
	 */
	public void setHoliday(Integer holiday) {
		this.holiday = holiday;
	}

	/**
	 * @return the hwStartTime
	 */
	public Date getHwStartTime() {
		return hwStartTime;
	}

	/**
	 * @param hwStartTime the hwStartTime to set
	 */
	public void setHwStartTime(Date hwStartTime) {
		this.hwStartTime = hwStartTime;
	}

	/**
	 * @return the hwCloseTime
	 */
	public Date getHwCloseTime() {
		return hwCloseTime;
	}

	/**
	 * @param hwCloseTime the hwCloseTime to set
	 */
	public void setHwCloseTime(Date hwCloseTime) {
		this.hwCloseTime = hwCloseTime;
	}

	/**
	 * @return the start
	 */
	public Date getStart() {
		return start;
	}

	/**
	 * @param start the start to set
	 */
	public void setStart(Date start) {
		this.start = start;
	}

	/**
	 * @return the end
	 */
	public Date getEnd() {
		return end;
	}

	/**
	 * @param end the end to set
	 */
	public void setEnd(Date end) {
		this.end = end;
	}

}
