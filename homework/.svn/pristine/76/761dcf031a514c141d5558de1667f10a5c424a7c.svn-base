package cn.strong.leke.homework.model.query;

import java.util.List;

import cn.strong.leke.homework.model.AnswerInfo;

public class ApiParamForSubmitBugFix {

	private Long homeworkDtlId;
	
	private List<AnswerInfo> answerJson;

	/**
	 * @return the homeworkDtlId
	 */
	public Long getHomeworkDtlId() {
		return homeworkDtlId;
	}

	/**
	 * @param homeworkDtlId the homeworkDtlId to set
	 */
	public void setHomeworkDtlId(Long homeworkDtlId) {
		this.homeworkDtlId = homeworkDtlId;
	}

	/**
	 * @return the answerJson
	 */
	public List<AnswerInfo> getAnswerJson() {
		return answerJson;
	}

	/**
	 * @param answerJson the answerJson to set
	 */
	public void setAnswerJson(List<AnswerInfo> answerJson) {
		this.answerJson = answerJson;
	}
}
