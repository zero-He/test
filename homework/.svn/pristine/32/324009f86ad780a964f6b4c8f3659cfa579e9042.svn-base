package cn.strong.leke.homework.model;

public class HomeworkPaperMQ implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1541422733822576103L;

	public HomeworkPaperMQ() {
	}

	public HomeworkPaperMQ(Long classId, Long schoolId,Integer type) {
		this.classId = classId;
		this.schoolId = schoolId;
		this.type = type;
	}

	/**
	 * 周 ：1
	 * 月：2
	 */
	private Integer type;
	/**
	 * 班级id
	 */
	private Long classId;

	private Long schoolId;

	/**
	 * @return the classId
	 */
	public Long getClassId() {
		return classId;
	}

	/**
	 * @param classId the classId to set
	 */
	public void setClassId(Long classId) {
		this.classId = classId;
	}

	/**
	 * @return the schoolId
	 */
	public Long getSchoolId() {
		return schoolId;
	}

	/**
	 * @param schoolId the schoolId to set
	 */
	public void setSchoolId(Long schoolId) {
		this.schoolId = schoolId;
	}

	/**
	 * @return the type
	 */
	public Integer getType() {
		return type;
	}

	/**
	 * @param type the type to set
	 */
	public void setType(Integer type) {
		this.type = type;
	}


	
}
