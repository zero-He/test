package cn.strong.leke.diag.model.teachingMonitor;

import java.io.Serializable;

public class HomePageSchedule implements Serializable{

	private static final long serialVersionUID = -6298385091984320303L;

	private Long courseSingleId;
	
	private String className;
	
	private String subjectName;
	
	private Long teacherId;
	
	private String teacherName;
	
	private String lessonTimeStr;
	
	private String startDate;
	
	//是否有录像
	private Integer isRecord;
	
	//课堂是否已结束，包括已上课和未上课
	private Integer isEnd;
	
	//是否有备课
	private Integer isPrepared;
	
	//课堂状态：1.已结束  2.未上课  3.待上课  4.上课中
	private String lessonStatus;
	
	//课件数
	private Integer cwCount;
	
	//微课数
	private Integer mcCount;
	
	//作业数
	private Integer hwCount;
	
	//教案数
	private Integer lessonPlan;

	public Long getCourseSingleId() {
		return courseSingleId;
	}

	public void setCourseSingleId(Long courseSingleId) {
		this.courseSingleId = courseSingleId;
	}

	public String getLessonTimeStr() {
		return lessonTimeStr;
	}

	public void setLessonTimeStr(String lessonTimeStr) {
		this.lessonTimeStr = lessonTimeStr;
	}

	public Integer getIsRecord() {
		return isRecord;
	}

	public void setIsRecord(Integer isRecord) {
		this.isRecord = isRecord;
	}

	public Integer getIsEnd() {
		return isEnd;
	}

	public void setIsEnd(Integer isEnd) {
		this.isEnd = isEnd;
	}

	public Integer getIsPrepared() {
		return isPrepared;
	}

	public void setIsPrepared(Integer isPrepared) {
		this.isPrepared = isPrepared;
	}

	public String getLessonStatus() {
		return lessonStatus;
	}

	public void setLessonStatus(String lessonStatus) {
		this.lessonStatus = lessonStatus;
	}

	public Integer getCwCount() {
		return cwCount;
	}

	public void setCwCount(Integer cwCount) {
		this.cwCount = cwCount;
	}

	public Integer getMcCount() {
		return mcCount;
	}

	public void setMcCount(Integer mcCount) {
		this.mcCount = mcCount;
	}

	public Integer getHwCount() {
		return hwCount;
	}

	public void setHwCount(Integer hwCount) {
		this.hwCount = hwCount;
	}

	public Integer getLessonPlan() {
		return lessonPlan;
	}

	public void setLessonPlan(Integer lessonPlan) {
		this.lessonPlan = lessonPlan;
	}

	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public String getSubjectName() {
		return subjectName;
	}

	public void setSubjectName(String subjectName) {
		this.subjectName = subjectName;
	}

	public String getTeacherName() {
		return teacherName;
	}

	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}

	public String getStartDate() {
		return startDate;
	}

	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}

	public Long getTeacherId() {
		return teacherId;
	}

	public void setTeacherId(Long teacherId) {
		this.teacherId = teacherId;
	}
	
}
