package cn.strong.leke.diag.model.report;

import java.util.List;

import cn.strong.leke.diag.dao.homework.model.LessonExamAnaly;

public class ReviewInfo {

	private Overall overall;
	private List<LessonExamAnaly> exams;
	private List<StudentInfo> details;

	public Overall getOverall() {
		return overall;
	}

	public void setOverall(Overall overall) {
		this.overall = overall;
	}

	public List<LessonExamAnaly> getExams() {
		return exams;
	}

	public void setExams(List<LessonExamAnaly> exams) {
		this.exams = exams;
	}

	public List<StudentInfo> getDetails() {
		return details;
	}

	public void setDetails(List<StudentInfo> details) {
		this.details = details;
	}

	public static class Overall {
		private Integer total;
		private Integer finish;
		private Integer portion;
		private Integer finish1;
		private Integer finish2;
		private Integer finish3;

		public Integer getTotal() {
			return total;
		}

		public void setTotal(Integer total) {
			this.total = total;
		}

		public Integer getFinish() {
			return finish;
		}

		public void setFinish(Integer finish) {
			this.finish = finish;
		}

		public Integer getPortion() {
			return portion;
		}

		public void setPortion(Integer portion) {
			this.portion = portion;
		}

		public Integer getFinish1() {
			return finish1;
		}

		public void setFinish1(Integer finish1) {
			this.finish1 = finish1;
		}

		public Integer getFinish2() {
			return finish2;
		}

		public void setFinish2(Integer finish2) {
			this.finish2 = finish2;
		}

		public Integer getFinish3() {
			return finish3;
		}

		public void setFinish3(Integer finish3) {
			this.finish3 = finish3;
		}
	}

	public static class StudentInfo {

		private Long userId;
		private String userName;
		private Integer state;
		private Integer state1 = -1;
		private Integer state2 = -1;
		private Integer state3 = -1;

		public Long getUserId() {
			return userId;
		}

		public void setUserId(Long userId) {
			this.userId = userId;
		}

		public String getUserName() {
			return userName;
		}

		public void setUserName(String userName) {
			this.userName = userName;
		}

		public Integer getState() {
			return state;
		}

		public void setState(Integer state) {
			this.state = state;
		}

		public Integer getState1() {
			return state1;
		}

		public void setState1(Integer state1) {
			this.state1 = state1;
		}

		public Integer getState2() {
			return state2;
		}

		public void setState2(Integer state2) {
			this.state2 = state2;
		}

		public Integer getState3() {
			return state3;
		}

		public void setState3(Integer state3) {
			this.state3 = state3;
		}
	}
}
