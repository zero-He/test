package cn.strong.leke.diag.model.teachingMonitor;

import java.io.Serializable;

public class LessonAttendInfo extends CommProp implements Serializable{
	
	private static final long serialVersionUID = 2639919084071549053L;

	//总课堂数
	private Long totalLesson;
	
	//实上课数
	private Long attendLesson;
	
	//未上课数
	private Long notAttendLesson;
	
	//上课率
	private String attendLessonRate = DEFAULT_RATE;
	
	//未上课率
	private String notAttendLessonRate = DEFAULT_RATE;

	public Long getTotalLesson() {
		return totalLesson;
	}

	public void setTotalLesson(Long totalLesson) {
		this.totalLesson = totalLesson;
	}

	public Long getAttendLesson() {
		return attendLesson;
	}

	public void setAttendLesson(Long attendLesson) {
		this.attendLesson = attendLesson;
	}

	public Long getNotAttendLesson() {
		return notAttendLesson;
	}

	public void setNotAttendLesson(Long notAttendLesson) {
		this.notAttendLesson = notAttendLesson;
	}

	public String getAttendLessonRate() {
		return attendLessonRate;
	}

	public void setAttendLessonRate(String attendLessonRate) {
		this.attendLessonRate = attendLessonRate;
	}

	public String getNotAttendLessonRate() {
		return notAttendLessonRate;
	}

	public void setNotAttendLessonRate(String notAttendLessonRate) {
		this.notAttendLessonRate = notAttendLessonRate;
	}
	
}
